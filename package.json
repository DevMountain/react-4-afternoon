{
  "name": "routing-afternoon",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.2",
    "json-server": "^0.15.1",
    "react": "^16.3",
    "react-dom": "^16.3"
  },
  "devDependencies": {
    "react-scripts": "1.0.11"
  },
  "scripts": {
    "start": "concurrently  \"react-scripts start\" \"json-server --watch db.json --port 3005 --delay 500\"",
